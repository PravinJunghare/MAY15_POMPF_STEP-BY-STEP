<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListener" />
		
		
	</listeners>

	<test name="Login Page Test">
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
		
	</test>
		<test name="Accounts Page Test">
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>
		<test name="ProductPageInfo Page Test">
		<classes>
			<class name="com.qa.opencart.tests.ProductPageInfoTest" />
		</classes>
	</test>
		<test name="Registration Page Test">
		<classes>
			<class name="com.qa.opencart.tests.RegistrationPageTest" />
		</classes>
	</test>
</suite>
	
	
	
	
	

